<h1>Public::Addresses#index</h1>
<p>Find me in app/views/public/addresses/index.html.erb</p>

<div class="container">
  <div class="row">
    
   <div class="col-mx-1">
            <h1>配送先登録</h1>
            <%= form_with model: @address, url:addresses_index_path, method: :post do |f| %>
            <%= f.label :宛名 %><br>
            <%= f.text_field :name, autofocus: true, autocomplete: "name" %>
            <br><br><br>
            <%= f.label :郵便番号 %><br>
            <%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code" %>
            <br><br><br>
            <%= f.label :住所 %><br>
            <%= f.text_field :address, autofocus: true, autocomplete: "address" %>
            <% end %>
          </tr>
      </div>
    </div>
  </div>